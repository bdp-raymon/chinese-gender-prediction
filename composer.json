{
    "name": "bdp-raymon/chinese-gender-prediction",
    "description": "Chinese Gender Prediction",
    "keywords": [
        "bdp-raymon",
        "chinese-gender-prediction"
    ],
    "homepage": "https://github.com/bdp-raymon/chinese-gender-prediction",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ali Shabani",
            "email": "alishabani9270@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "BdpRaymon\\ChineseGenderPrediction\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BdpRaymon\\ChineseGenderPrediction\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
